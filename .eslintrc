{
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "node": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "prefer-const": "warn",
    "no-var": "error",
    "no-async-promise-executor": "off",
    "require-atomic-updates": "off",
    "no-prototype-builtins": "off"
  }
}
